<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition Height="45"></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="45"></RowDefinition>
        </Grid.RowDefinitions>

        <TextBox Grid.Row="2" Name ="Txbt1" TextWrapping="Wrap" HorizontalAlignment="Right" Width="137" Height="30" VerticalAlignment="Top"/>
        <Frame Name="Fr" Grid.Row="1" NavigationUIVisibility="Hidden" Navigated="MainFrame_OnNavigated"/>
        <Button Name="Backbtn" Content="Назад" Grid.Row="0" HorizontalAlignment="Left"  Width="71" Click="Backbtn_Click" />

    </Grid>
</Window>